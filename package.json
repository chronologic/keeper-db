{
  "name": "keeper-db",
  "version": "1.0.0",
  "description": "DB schema for Keeper",
  "main": "dist/index.js",
  "scripts": {
    "eslint": "eslint src/index.ts",
    "build": "tsc",
    "lint": "./node_modules/eslint/bin/eslint.js  src --ext .ts",
    "db-sync": "cross-env DB_SYNC=true ts-node ./src/dbSync.ts",
    "fix-lint": "./node_modules/eslint/bin/eslint.js  src --ext .ts --fix",
    "build-release": "git checkout release && npm run build"
  },
  "dependencies": {
    "connection-string-parser": "^1.0.3",
    "dotenv": "^8.2.0",
    "ethers": "^5.0.29",
    "pg": "^8.5.1",
    "typeorm": "^0.2.30",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/node": "^14.14.25",
    "cross-env": "^7.0.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
